{
  "version": 2,
  "name": "sidi-hackathon",
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "frontend",
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
